{
	"multipart": [
		{
			"apply": {
				"model": "columns:block/granite_column_center"
			}
		},
		{
			"when": {
				"down": true
			},
			"apply": {
				"model": "columns:block/granite_column_end"
			}
		},
		{
			"when": {
				"up": true
			},
			"apply": {
				"model": "columns:block/granite_column_end",
				"x": 180,
				"uvlock": true
			}
		}
	]
}
